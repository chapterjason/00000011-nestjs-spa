{
  "name": "structure-one",
  "version": "0.1.0-DEV",
  "license": "MIT",
  "private": true,
  "scripts": {
    "frontend:lint": "eslint -c ./assets/.eslintrc ./assets/js/**/*.tsx ./assets/js/**/*.ts",
    "frontend:dev:build": "encore dev",
    "frontend:dev:watch": "encore dev --watch",
    "frontend:dev:server": "encore dev-server",
    "frontend:prod:build": "encore production",
    "backend:lint": "eslint -c .eslintrc src/**/*.ts",
    "backend:build": "tsc",
    "backend:start": "nodemon",
    "backend:migrate": "ts-node node_modules/typeorm/cli.js migration:run",
    "docker:start": "docker-compose -f ./docker-compose.yaml up -d",
    "docker:stop": "docker-compose -f ./docker-compose.yaml down --remove-orphans --volumes",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-react": "^7.0.0",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@mscs/eslint-config": "^0.1.2",
    "@symfony/webpack-encore": "^0.29.1",
    "@types/node": "^13.13.2",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-router-dom": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "core-js": "^3.6.5",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "jquery": "^3.5.0",
    "node-sass": "^4.14.0",
    "popper.js": "^1.16.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-use": "^14.1.1",
    "sass-loader": "^7.0.1",
    "ts-loader": "^5.3.0",
    "typescript": "^3.8.3",
    "webpack-notifier": "^1.6.0"
  },
  "dependencies": {
    "@mscs/environment": "^0.1.0",
    "@nestjs/common": "^7.0.9",
    "@nestjs/core": "^7.0.9",
    "@nestjs/platform-express": "^7.0.9",
    "@nestjs/typeorm": "^7.0.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "iterare": "^1.2.0",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.3",
